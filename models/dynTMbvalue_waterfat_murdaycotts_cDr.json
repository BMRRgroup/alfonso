    "add_scan_param": ['DT', 'gr_dur_s', 'gr_str_magn_Tm', 'gamma_bar'],
    "bvalue_func": "bvalue_func = @(D,r) ALFONSO.model_murdaycotts(r(:),D(:),DT,gr_str_magn_Tm,gr_dur_s,gamma_bar);",
    "add_model_param": ['r'],
    "r_vars": [24, 24, 24, 24, 24, 24, 24, 24, 24, 24, 25],
    "r_constraint": "@(x) ALFONSO.bound_tanh(x(modelparam.r_vars), 1e-7, 1e-5)",
    "r_inv_constraint": "@(x) ALFONSO.bound_inv_tanh(x(modelparam.r_vars), 1e-7, 1e-5)",
    "r_init": [1e-6, 1e-6, 1e-6, 1e-6, 1e-6, 1e-6, 1e-6, 1e-6, 1e-6, 1e-6, 1e-6]
