{
    "model_name": "model_template",
    "description": "This template includes all parameters that can be modeled. It is not functional! Delete a parameters as needed. 
Bounds (_lb,_ub) and _constraint cannot be used simultanously.
The Theta functions are given for reference and can be removed if remained unchanged.",
    "author": " (John Doe john.doe@email.com)",
    "created_date": "01-Jan-1970",
    "last_update_date": "01-Jan-2000",
    "algorithm": "levenberg-marquardt",
    "rho_vars": [1, 2],
    "rho_init": [0.5],
    "rho_constraint": "@(x) exp(x(modelparam.rho_vars))",
    "rho_inv_constraint": "@(x) log(x(modelparam.rho_vars))",
    "rho_lb": [0],
    "rho_ub": [Inf],
    "omega_ref": [1.30, 4.67],
    "omega_vars": [3, 4],
    "omega_init": [0],
    "omega_constraint": "@(x) ALFONSO.bound_tanh(x(modelparam.omega_vars), -0.2, 0.2)",
    "omega_inv_constraint": "@(x) ALFONSO.bound_inv_tanh(x(modelparam.omega_vars), -0.2, 0.2)",
    "omega_lb": [-0.2],
    "omega_ub": [0.2],
    "phi_vars": [5],
    "phi_init": [0],
    "phi_constraint": "@(x) ALFONSO.bound_tanh(x(modelparam.phi_vars), -0.2, 0.2)",
    "phi_inv_constraint": "@(x) ALFONSO.bound_inv_tanh(x(modelparam.phi_vars), -0.2, 0.2)",
    "phi_lb": [-0.2],
    "phi_ub": [0.2],
    "d_vars": [6, 7],
    "d_init": [0.01],
    "d_constraint": "@(x) exp(x(modelparam.d_vars))",
    "d_inv_constraint": "@(x) log(x(modelparam.d_vars))",
    "d_lb": [0],
    "d_ub": [Inf],
    "g_vars": [6, 7],
    "g_init": [0.01],
    "g_constraint": "@(x) exp(x(modelparam.g_vars))",
    "g_inv_constraint": "@(x) log(x(modelparam.g_vars))",
    "g_lb": [0],
    "g_ub": [Inf],
    "T1_vars": [8, 9],
    "T1_init": [0.700, 1.200],
    "T1_constraint": "@(x) exp(x(modelparam.T1_vars))",
    "T1_inv_constraint": "@(x) log(x(modelparam.T1_vars))",
    "T1_lb": [0],
    "T1_ub": [Inf],
    "T2_vars": [10, 11],
    "T2_init": [0.070, 0.015],
    "T2_constraint": "@(x) exp(x(modelparam.T2_vars))",
    "T2_inv_constraint": "@(x) log(x(modelparam.T2_vars))",
    "T2_lb": [0],
    "T2_ub": [Inf],
    "add_model_param": ['myparam'],
    "myparam_vars": [12, 13],
    "myparam_constraint": "@(x) exp(x(modelparam.myparam_vars))",
    "myparam_inv_constraint": "@(x) log(x(modelparam.myparam_vars))",
    "myparam_init": [1e-6, 1e-6],
    "add_scan_param": ['gamma_bar'],
    "TI_func": "TI_func = @(T1) ( 1 - 2 .* exp(- shiftdim(TI(:),-2) ./ ensure_row(T1)) );",
    "TE_func": "TE_func = @(T2) ( exp( - shiftdim(TE(:),-3) ./ ensure_row(T2)) );",
    "TM_func": "TM_func = @(T1) exp( - shiftdim(TM(:),-4) ./ ensure_row(T1) );",
    "bvalue_func": "bvalue_func = bvalue_func = @(D) (exp(- ensure_row(D) .* shiftdim(bvalue(:),-5)));"{}
}